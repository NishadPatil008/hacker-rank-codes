#include <stdio.h>

void update(int *a, int *b) {
    int sum = *a + *b;     // store sum of both numbers
    int diff = *a - *b;    // store difference
    if (diff < 0) diff = -diff;  // make it absolute
    *a = sum;              // update a with sum
    *b = diff;             // update b with absolute difference
}

int main() {
    int a, b;
    int *pa = &a, *pb = &b;
    
    scanf("%d %d", &a, &b);
    update(pa, pb);
    printf("%d\n%d", a, b);
    
    return 0;
}
