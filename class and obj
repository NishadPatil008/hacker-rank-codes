

class Student {
    vector<int> scores;
public:
    void input() {
        for (int i = 0, x; i < 5; i++) {
            cin >> x;
            scores.push_back(x);
        }
    }
    int calculateTotalScore() {
        int sum = 0;
        for (int x : scores) sum += x;
        return sum;
    }
};

